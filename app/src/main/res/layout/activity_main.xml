<?xml version="1.0" encoding="utf-8"?>



<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dlo_drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorSoso">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/rlo_toolBar"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <ImageButton
                android:id="@+id/ibtn_openDrawer"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:background="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/ic_menu" />

            <ImageButton
                android:id="@+id/ibtn_map"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentTop="true"

                android:layout_marginTop="10dp"
                android:layout_toLeftOf="@id/ibtn_share"
                android:background="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/ic_map_icon"
                android:layout_marginHorizontal="10dp"
                />

            <ImageButton
                android:id="@+id/ibtn_share"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentTop="true"

                android:layout_marginTop="10dp"
                android:layout_toLeftOf="@id/ibtn_bookMark"
                android:background="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/ic_share_icon" />

            <ImageButton
                android:id="@+id/ibtn_bookMark"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_marginHorizontal="10dp"
                android:layout_marginTop="10dp"
                android:background="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/ic_plus_icon" />
        </RelativeLayout>
        <androidx.viewpager.widget.ViewPager
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/viewPager"
            >
        </androidx.viewpager.widget.ViewPager>
    </LinearLayout>


    <LinearLayout
        android:layout_width="270dp"
        android:background="#ffffff"
        android:layout_height="match_parent"
        android:layout_gravity="left"
        android:orientation="vertical">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="170dp"
            android:scaleType="fitXY"
            android:src="@drawable/mise" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s1" />

        <ImageButton
            android:id="@+id/ibtn_who"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s2" />

        <ImageButton
            android:id="@+id/ibtn_eight"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s3" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s4" />

        <ImageButton
            android:id="@+id/btn_info"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s5" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s6" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s7" />

        <ImageButton
            android:id="@+id/ibtn_setting"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s8" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:paddingLeft="0dp"
            android:paddingTop="0dp"
            android:paddingRight="0dp"
            android:paddingBottom="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/s9" />
    </LinearLayout>



</androidx.drawerlayout.widget.DrawerLayout>


